
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// TODO: Update schema
model Experiment {
  id        String   @id @default(uuid())
  prompt    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("experiments")
}
