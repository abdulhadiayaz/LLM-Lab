{
  "name": "llm-lab-server",
  "version": "1.0.0",
  "description": "Server for LLM LAB challenge",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=local nodemon --ignore prisma/generated/ --ext ts --exec 'npm run gen-client && tsx index.ts'",
    "build": "npm run gen-client && tsup index.ts",
    "gen-client": "prisma generate",
    "lint": "eslint ./src && node --max-old-space-size=4096 ../node_modules/.bin/tsc --project tsconfig.json --noEmit",
    "start": "cross-env NODE_ENV=production node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.8.1",
    "axios": "^1.6.7",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-zod-api": "^16.6.0",
    "natural": "^8.1.0",
    "prisma": "^5.8.1",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "uuid": "^10.0.0",
    "zod": "^3.23",
    "zod-prisma-types": "^3.1.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/node": "^20.11.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "tsconfig-paths": "^4.2.0"
  }
}
